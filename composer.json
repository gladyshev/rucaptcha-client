{
  "name": "gladyshev/rucaptcha-client",
  "keywords": ["captcha recognition", "rucaptcha"],
  "description": "Useful rucaptcha.com PHP-client with PSR-7 and PSR-3 support.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.ru",
      "homepage": "https://github.com/gladyshev"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-SimpleXML": "*",
    "psr/http-client": "~1.0.0",
    "psr/log": "~1.0.0",
    "guzzlehttp/guzzle": "~7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Rucaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rucaptcha\\Test\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
