{
  "name": "gladyshev/rucaptcha-client",
  "keywords": ["captcha recognition", "rucaptcha"],
  "description": "This is a modern full-featured RuCaptcha PHP-client with PSR-7 and PSR-3 support.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.ru",
      "homepage": "https://github.com/gladyshev"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "~6.2",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Rucaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rucaptcha\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
