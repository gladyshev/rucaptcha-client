{
  "name": "gladyshev/rucaptcha-client",
  "keywords": ["captcha recognition", "rucaptcha", "2captcha.com", "rucaptcha.com"],
  "description": "Lightweight and useful 2captcha.com/rucaptcha.com PHP-client with PSRs support.",
  "type": "library",
  "license": "Unlicense",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.ru"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/http-client": "~1.0",
    "psr/log": "~1|~2|~3",
    "guzzlehttp/psr7": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.6",
    "squizlabs/php_codesniffer": "3.5.2",
    "monolog/monolog": "2|3",
    "guzzlehttp/guzzle": "~7.2"
  },
  "autoload": {
    "psr-4": {
      "Rucaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rucaptcha\\Test\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
